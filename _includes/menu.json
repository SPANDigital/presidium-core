{
  logo : "{{ site.logo }}",
  brandName : "{{ site.name }}",
  baseUrl :  {% if site.baseurl == "" %} "/" {% else %} "{{ site.baseurl }}/" {% endif %},
  currentPage : "{{ site.baseurl | append: page.url | replace: '//','/'}}",
  roles: { {% if site.roles != nil %}
              label: "{{site.roles.label}}",
              all: "{{site.roles.all}}",
